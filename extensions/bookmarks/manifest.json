{
    "manifest_version": 3,
    "name": "Bookmark Search",
    "version": "0.0.0",
  
    "description": "This ",
  
    "author": "Yannik Wyss",

    "icons": {
        "16": "icons/bookmark_16.png",
        "48": "icons/bookmark_48.png"
    },

    "incognito": "spanning",

    "offline_enabled": true,

    "short_name": "Bookmark",

    "omnibox": { "keyword" : "." },

    "permissions": [
        "bookmarks",
        "tabs"
    ],

    "background": {
        "service_worker": "background.js"
    }
  }